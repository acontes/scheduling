<highlighters>

	<highlighter type='oneline-comment'>
	  #
	</highlighter>

	<highlighter type='string'>
		<string>`</string>
		<escape>\</escape>
	</highlighter>

	<highlighter type='string'>
		<string>"</string>
		<escape>\</escape>
	</highlighter>

	<highlighter type='string'>
		<string>'</string>
		<escape>\</escape>
	</highlighter>

	<highlighter type='keywords'>
		<keyword>if</keyword>
		<keyword>for</keyword>
		<keyword>do</keyword>
		<keyword>done</keyword>
		<keyword>cd</keyword>
		<keyword>fi</keyword>
		<keyword>then</keyword>
		<keyword>test</keyword>
		<keyword>echo</keyword>
		<keyword>exit</keyword>
		<keyword>shift</keyword>
		<keyword>case</keyword>
		<keyword>esac</keyword>
		<keyword>in</keyword>
		<keyword>set</keyword>
		<keyword>export</keyword>
	</highlighter>

</highlighters>
