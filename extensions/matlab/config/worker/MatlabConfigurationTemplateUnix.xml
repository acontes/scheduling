<?xml version="1.0" encoding="ISO-8859-15"?>
<!-- Rename this file to MatlabConfiguration.xml to configure matlab installations on the current machine -->
<MatlabInstallations>
    <installation>
        <version>7.9</version> <!-- matlab version in numeric format -->
        <home>/usr/local/matlab2009b</home> <!-- Matlab home directory -->
        <bindir>bin</bindir> <!-- relative path to bin directory which contains the matlab executable -->
        <command>matlab</command> <!-- matlab command name (could be matlab or matlab.exe -->
    </installation>
</MatlabInstallations>