#!/usr/bin/env bash

CLASSPATH=.
workingDir=`dirname $0`
. $workingDir/env scheduler-log4j-server

yjp=-agentlib:yjpagent
opt="-Xms128m -Xmx2048m"

eval $JAVACMD -Dderby.stream.error.file=\"$PA_SCHEDULER/.logs/derby.log\" $opt org.ow2.proactive.scheduler.util.SchedulerStarter $@

retCode=$?
echo
exit $retCode
