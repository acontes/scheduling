#!/usr/bin/env bash

if [ $# -lt 1 ]; then
	echo
	echo "using default values. -h to display help message."
	echo
echo
fi

echo --- StartNode -------------------------------------------
echo

workingDir=`dirname $0`
. $workingDir/env log4j-defaultNode
eval $JAVACMD org.ow2.proactive.resourcemanager.utils.RMNodeStarter "$@"

retCode=$?
echo
echo ---------------------------------------------------------
exit $retCode
