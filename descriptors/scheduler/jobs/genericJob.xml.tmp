<?xml version="1.0" encoding="UTF-8"?>
<job name="..." priority="lowest/low/normal/high/highest" cancelOnException="true/false">
	<!-- priority : optional -> default : normal -->
	<!-- cancelOnException : optional -> default : false? -->
	<description>...</description>
	<taskFlow>
	
		<tasks>
			<task name="..." preciousResult="true/false" retries="4" depends="... ... ...">
