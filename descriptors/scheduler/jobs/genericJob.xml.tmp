				<!-- retries : optional -> default : 0 -->
				<!-- depends : optional -->
				<description>...</description>
				<!-- selectionScript, preScript, postScript : optional -->
				<selectionScript>
					<file url="..." path="..."><!-- url OR path -->
						<arguments>
							<argument value="..." />
							<argument value="..." />
						</arguments>
					</file>
					<!-- OR -->
					<code language="...">
						<!-- code -->
					</code>
				</selectionScript>
				
				<preScript>
					<file url="..." path="..."><!-- url OR path -->
						<arguments>
							<argument value="..." />
							<argument value="..." />
						</arguments>
					</file>
					<!-- OR -->
					<code language="...">
						<!-- code -->
					</code>
				</preScript>
				
				<javaExecutable class="...">
					<parameters>
						<parameter name="..." value="..." />
						<parameter name="..." value="..." />
					</parameters>
				</javaExecutable>
				
				<postScript>
					<file url="..." path="..."><!-- url OR path -->
						<arguments>
							<argument value="..." />
							<argument value="..." />
						</arguments>
					</file>
					<!-- OR -->
					<code language="...">
						<!-- code -->
					</code>
				</postScript>
				
			</task>
			
			
			
			<task name="...">
				<description>...</description>
				
				<selectionScript>
					<file url="..." path="..."><!-- url OR path -->
						<arguments>
							<argument value="..." />
							<argument value="..." />
						</arguments>
					</file>
				</selectionScript>
				
				<preScript>
					<code language="...">
						<!-- code -->
					</code>
				</preScript>
				
				<nativeExecutable>
					<staticCommand value="...">
						<arguments>
							<argument value="..." />
							<argument value="..." />
						</arguments>
					</staticCommand>
					<!-- OR -->
					<dynamicCommand>
						<generationScript>
							<!-- script, url, path, ... -->
						</generationScript>
					</dynamicCommand>
				</nativeExecutable>
				
				<postScript>
					<code language="...">
						<!-- code -->
					</code>
				</postScript>
				
			</task>
			
			<!-- ... -->
			
		</tasks>
	</taskFlow>
	
	
	
	<!-- other job type (WARNING : only one *job per 'job' root mark) -->
	<description>...</description>
	<proActive neededNodes="4">
		<task name="...">
			<description>...</description>
			
			<!-- selectionScript, preScript : optional -->
			
			<proActiveExecutable class="...">
				<parameters>
					<parameter name="..." value="..." />
					<parameter name="..." value="..." />
				</parameters>
			</proActiveExecutable>
			
			<!-- postScript : optional -->
				
		</task>
	</proActive>
</job>