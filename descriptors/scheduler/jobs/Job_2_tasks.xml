<?xml version="1.0" encoding="UTF-8"?>
<job xmlns="urn:proactive:jobdescriptor:0.91" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:proactive:jobdescriptor:0.91 http://proactive.inria.fr/schemas/jobdescriptor/0.91/schedulerjob.xsd"
	id="job_2_tasks" priority="low" projectName="myProject" cancelOnError="true">
	<description>2 tasks with variable durations</description>
	<genericInformation>
		<info name="var1" value="val1"/>
		<info name="var2" value="val2"/>
	</genericInformation>
	<taskFlow>
		<task id="task1" retries="3">
			<!-- Task human description -->
			<description>task WaitAndPrint - will sleep for 3s</description>
			<genericInformation>
				<info name="name1" value="val1"/>
			</genericInformation>
			<javaExecutable class="org.objectweb.proactive.extensions.scheduler.examples.WaitAndPrint">
				<parameters>
					<parameter name="sleepTime" value="3"/>
					<parameter name="number" value="1"/>
				</parameters>
			</javaExecutable>
		</task>
		<task id="task2">
			<!-- Task human description -->
			<description>task WaitAndPrint - will sleep for 20s</description>
			<javaExecutable class="org.objectweb.proactive.extensions.scheduler.examples.WaitAndPrint">
				<parameters>
					<parameter name="sleepTime" value="20"/>
					<parameter name="number" value="2"/>
				</parameters>
			</javaExecutable>
		</task>
	</taskFlow>
</job>
