<?xml version="1.0" encoding="UTF-8"?>
<project name="A Scheduler for ProActive" basedir="../../../../../../../compile/">

	<import file="${basedir}/build.xml" />
	<target name="compile" depends="core, stubGenerator,-compile"/>
	<target name="-compile">
		<if>
			<bool>
				<contains string="${java.version}" substring="1.5"/>
			</bool>
			<echo>Scheduler requires Java 6 or newer ;  Skipping scheduler compilation...</echo>
			<else>
				<compile_extra module="scheduler" />
				<!--<java classname="${stubgenerator.class}" fork="true">
					<classpath refid="project.classpath" />
					<arg line="-srcDir ${cl.extra.dir} -pkg org.objectweb.proactive.extra.scheduler" />
				</java>-->
			</else>
		</if>
	</target>
</project>
