<?xml version="1.0" encoding="UTF-8"?>
<job xmlns="urn:proactive:jobdescriptor:dev" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:proactive:jobdescriptor:dev ../../../src/org/ow2/proactive/scheduler/common/xml/schemas/jobdescriptor/dev/schedulerjob.xsd"
	name="job_proActive" priority="low">
	<description>A ProActive job that will find out the Nth first prime number where N is given as
		argument.</description>
	<proActive neededNodes="5">
		<task name="Controller">
			<description>Will control the workers in order to find the prime number</description>
			<proActiveExecutable
				class="org.ow2.proactive.scheduler.examples.ProActiveExample">
				<parameters>
					<parameter name="numberToFind" value="100"/>
				</parameters>
			</proActiveExecutable>
		</task>
	</proActive>
</job>
