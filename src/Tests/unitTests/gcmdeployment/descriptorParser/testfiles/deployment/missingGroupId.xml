<?xml version="1.0" encoding="UTF-8"?>
<GCMDeployment 
	xmlns="urn:gcm:deployment:1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:gcm:deployment:1.0 http://proactive.inria.fr/schemas/gcm/1.0/ExtensionSchemas.xsd	">

	<resources>
		<group refid="WRONG_ID">
			<host refid="myhost" />
		</group>
	</resources>

	<infrastructure>
	
		<groups>
			<sshGroup id="mygroup" hostList="host" />
		</groups>

		<hosts>
			<host id="myhost" os="unix">
				<homeDirectory base="root" relpath="somewhere" />
			</host>
		</hosts>

	</infrastructure>

</GCMDeployment>
