		<!-- ***************************************************** -->
		<!-- P2P PROPERTIES VALUE -->
		<!-- THESE ARE DEFAULT VALUES -->
		
		<!-- Acquisition method -->
		<prop key="proactive.p2p.acq" value="rmi" />
		<!-- Port number -->
		<prop key="proactive.p2p.port" value="2901" />
		<!-- NOA is in number of peers -->
		<prop key="proactive.p2p.noa" value="10" />
		<!-- TTU is in ms -->
		<prop key="proactive.p2p.ttu" value="600000" />
		<!-- TTL is in hops -->
		<prop key="proactive.p2p.ttl" value="3" />
		<!-- List capacity of message sequence number -->
		<prop key="proactive.p2p.msg_capacity" value="1000" />
		<!-- Percentage of agree responses -->
		<prop key="proactive.p2p.expl_msg" value="75" />
		<!-- Timeout for node acquisition -->
		<prop key="proactive.p2p.nodes_acq_to" value="1200000" />
		<!-- Lookup frequency for nodes -->
		<prop key="proactive.p2p.lookup_freq" value="600000" />
		<!-- If true deploying one shared nodes by CPU, else only one node is shared -->
		<prop key="proactive.p2p.multi_proc_nodes" value="false" />
		<!-- Timeout for waiting node ack. -->
		<prop key="proactive.p2p.ack_to" value="600000" />
		<!-- Enable load balance -->
		<prop key="proactive.p2p.loadbalancing" value="false" />
		<!-- ***************************************************** -->

		<!-- Workaround to fix rmissh/unicore conflict -->
		<prop key="proactive.unicore.forkclient" value="true" />
